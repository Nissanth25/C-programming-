#include <stdio.h>
void main()
{
    int a=0,b=1,n;
    printf("Enter a number:");
    scanf("%d",&n);
    printf("fibronacci series: ");
    for(int i=0;i<=n;i++) 
{
  printf("%d", fib(i));
}
}
int fib(int n) 
{
 if(n<=1) {
 return n;
}
else{
  return fib(n-1) +fib(n-2) ;
 }
}

************************************************

Enter a number:6
The factorial is 0 1 1 2 3 5 8
